{
  "permissions": {
    "allow": [
      "Shell(ls)",
      "Shell(find)",
      "Shell(grep)",
      "Shell(rg)",
      "Shell(cat)",
      "Shell(head)",
      "Shell(tail)",
      "Shell(wc)",
      "Shell(tree)",
      "Shell(pwd)",
      "Shell(echo)",
      "Shell(git)",
      "Shell(npm)",
      "Shell(pip)",
      "Shell(python)",
      "Shell(node)",
      "Shell(curl)",
      "Shell(wget)",
      "Shell(lynx)",
      "Shell(jq)",
      "Read(**/*)",
      "Write(src/**)",
      "Write(tests/**)",
      "Write(docs/**)",
      "Write(scripts/**)",
      "Write(*.py)",
      "Write(*.ts)",
      "Write(*.tsx)",
      "Write(*.js)",
      "Write(*.jsx)",
      "Write(*.md)"
    ],
    "deny": [
      "Shell(rm -rf /)",
      "Shell(sudo)",
      "Shell(chmod)",
      "Shell(chown)",
      "Read(.env*)",
      "Read(**/.env*)",
      "Read(**/credentials*)",
      "Read(**/*.key)",
      "Read(**/*.pem)",
      "Read(**/*.p12)",
      "Write(.env*)",
      "Write(**/.env*)",
      "Write(**/credentials*)",
      "Write(**/*.key)",
      "Write(**/*.pem)",
      "Write(package-lock.json)",
      "Write(yarn.lock)"
    ]
  }
}
